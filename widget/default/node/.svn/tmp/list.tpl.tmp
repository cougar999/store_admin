			<div class="datatable">
				<!--{if $assign.page.items}-->
				<form action="/node?action=multiNodesAction" method="post" onsubmit="" id="nodeform">
				<div id="TablePanel" class="datatablecontent">
					<table id="tableviewer" width="100%" border="0">
						<colgroup>
							<col width="auto" />
							<col width="60" />
							<col width="80" />
							<col width="100" />
							<col width="80" />
							<col width="auto" />
							<col width="80" />
						</colgroup>
						<thead>
							<tr class="title">
								<th class="tc">名称</th>
								<th class="tc">ID</th>
								<th class="tc">图标</th>
